struct Node* deleteDuplicates(struct Node* head)
{
    if (head == NULL)
        return NULL;

    struct Node* curr = head;

    while (curr->next != NULL)
    {
        if (curr->val == curr->next->val)
        {
            curr->next = curr->next->next;
        }
        else
        {
            curr = curr->next;
        }
    }

    return head;
}
